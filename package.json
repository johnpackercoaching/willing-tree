  "type": "module",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:vercel": "vite build",
    "build:gh-pages": "vite build",
    "build:strict": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "test:headed": "npx playwright test --headed",
    "deploy": "npm run build:gh-pages && gh-pages -d dist",
    "cap:add": "npx cap add",
    "cap:copy": "npx cap copy",
    "cap:sync": "npx cap sync",
    "cap:run": "npx cap run",
    "ios": "npm run build && npx cap copy ios && npx cap run ios",
    "android": "npm run build && npx cap copy android && npx cap run android",
    "build:prod": "npm run lint && npm run build && npm run cap:sync"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
